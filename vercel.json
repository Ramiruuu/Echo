    {
    "version": 2,
    "builds": [
        {
        "src": "*.html",
        "use": "@vercel/static"
        },
        {
        "src": "static/**/*",
        "use": "@vercel/static"
        }
    ],
    "routes": [
        {
        "src": "/",
        "dest": "/index.html"
        },
        {
        "src": "/logs",
        "dest": "/logs.html"
        },
        {
        "src": "/settings", 
        "dest": "/settings.html"
        },
        {
        "src": "/static/(.*)",
        "dest": "/static/$1"
        }
    ]
    }